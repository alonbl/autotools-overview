PACKAGE_TARNAME = @PACKAGE_TARNAME@
EXEEXT = @EXEEXT@
CC = @CC@
INSTALL = @INSTALL@
DEFS = @DEFS@
CPPFLAGS = @CPPFLAGS@
CFLAGS = @CFLAGS@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@
prefix = @prefix@
exec_prefix = @prefix@
bindir = @bindir@
datarootdir = @datarootdir@
docdir = @docdir@

all:		\
		prog1 \
		$(NULL)

clean:
	rm -f prog1

install:	all
	$(INSTALL) -d -m 755 "$(DESTDIR)$(bindir)"
	$(INSTALL) -m 755 prog1$(EXEEXT) "$(DESTDIR)$(bindir)"
	$(INSTALL) -d -m 755 "$(DESTDIR)$(docdir)"
	$(INSTALL) -m 644 README.md "$(DESTDIR)$(docdir)"

uninstall:
	rm -f "$(DESTDIR)$(bindir)/prog1$(EXEEXT)"
	rm -f "$(DESTDIR)$(docdir)/README.md"

prog1:		\
		main.c \
		common.c \
		$(NULL)
	$(CC) -o $@$(EXEEXT) $(CPPFLAGS) $(CFLAGS) $(DEFS) $^ $(LDFLAGS) $(LIBS)
